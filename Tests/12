class Rect is
	width: Integer(0)
	height: Integer(0)
	this(arg1: Integer, arg2: Integer) is
		this.width := arg1
		this.height := arg2
	end
	
	method Perimeter(): Integer is
		return this.width.Plus(this.height).Mult(2)
	end
	
	method Square(): Integer
		return this.width.Mult(this.height)
	end
end
